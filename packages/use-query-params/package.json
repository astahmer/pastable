{
    "name": "@pastable/use-query-params",
    "description": "A collection of pastable code gathered from past projects.",
    "version": "0.0.1",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "exports": "./dist/index.modern.js",
    "module": "dist/index.module.js",
    "unpkg": "dist/index.umd.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/pastable.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "scripts": {
        "build": "microbundle build",
        "dev": "microbundle watch",
        "test": "uvu -r @swc-node/register -r tsconfig-paths/register src",
        "tw": "watchlist src -- yarn test",
        "coverage": "c8 --all yarn test",
        "coverage:open": "yarn scripts:openCoverageInBrowser",
        "coverage:view": "c8 --reporter=lcov --all yarn test && yarn coverage:open",
        "release": "standard-version",
        "prebuild": "rimraf dist",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test",
        "release-publish": "npm publish && git push --follow-tags origin main"
    },
    "devDependencies": {
        "@pastable/utils": "workspace:packages/utils",
        "@swc-node/register": "^1.0.5",
        "@testing-library/react-hooks": "^5.1.1",
        "@types/react": "^17",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-test-renderer": "^17",
        "@types/rimraf": "^3",
        "c8": "^7.7.1",
        "microbundle": "^0.13.0",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "standard-version": "^9.2.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4",
        "uvu": "^0.5.1",
        "watchlist": "^0.2.3"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-router-dom": "^5.2.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": "^14.x"
    },
    "keywords": [
        "pastable",
        "snippets",
        "reusable",
        "utils",
        "tiny",
        "lodash",
        "gists",
        "typescript",
        "react",
        "react-router",
        "query",
        "param",
        "query-param",
        "hook",
        "custom",
        "state"
    ]
}

{
    "name": "pastable",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/pastable.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "main": "dist/pastable.cjs.js",
    "module": "dist/pastable.esm.js",
    "files": [
        "src",
        "dist",
        "react",
        "utils",
        "machines",
        "typings",
        "server"
    ],
    "version": "2.0.11",
    "preconstruct": {
        "entrypoints": [
            "index.ts",
            "react.ts",
            "utils.ts",
            "machines.ts",
            "typings.ts",
            "server.ts"
        ]
    },
    "scripts": {
        "build": "pnpm preconstruct build",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "release": "pnpm build && pnpm publish && git push"
    },
    "packageManager": "pnpm@6.24.0",
    "sideEffects": false,
    "devDependencies": {
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@preconstruct/cli": "^2.2.1",
        "@types/node": "^17.0.45",
        "@types/prettier": "^2.7.0",
        "@types/react": "^17.0.48",
        "@types/react-dom": "^17.0.17",
        "@types/rimraf": "^3.0.2",
        "c8": "^7.12.0",
        "esno": "^0.16.3",
        "mitata": "0.1.5",
        "prettier": "^2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.4",
        "vitest": "^0.12.10",
        "xstate": "^4.33.0"
    },
    "peerDependencies": {
        "react": ">=17",
        "xstate": ">=4.32.1"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "xstate": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=14.x"
    },
    "keywords": [
        "pastable",
        "snippets",
        "reusable",
        "utils",
        "tiny",
        "lodash",
        "gists",
        "typescript",
        "react"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@babel/core": "^7.18.10",
        "ts-toolbelt": "^9.6.0",
        "type-fest": "^2.18.0"
    }
}
